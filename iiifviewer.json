{
  "id": "iiifviewer",

  "inputs": ["src/iiifviewer.js", "ol3js/externs/olx.js"],
  "paths": ["src", "ol3js/src"],

  "define": {
    "goog.DEBUG": false,
    "ol.ENABLE_PROJ4JS": false,
    "ol.ENABLE_VECTOR": false,
    "ol.ENABLE_WEBGL": false,
    "ol.ENABLE_IMAGE": false
  },

  "externs": [
    "ol3js/externs/bingmaps.js",
    "ol3js/externs/geojson.js",
    "ol3js/externs/oli.js",
    "ol3js/externs/proj4js.js",
    "ol3js/externs/tilejson.js",
    "ol3js/externs/closure-compiler.js"
  ],

  "mode": "ADVANCED",

  "output-wrapper": "/*\n * IiifViewer; Copyright (C) 2014 - Klokan Technologies GmbH\n * Powered by OpenLayers 3\n */\n%output%",

  "checks": {
    "checkTypes": "OFF",
    "checkRegExp": "ERROR",
    "checkVars": "ERROR",
    "deprecated": "ERROR",
    "fileoverviewTags": "ERROR",
    "invalidCasts": "OFF",
    "missingProperties": "ERROR",
    "nonStandardJsDocs": "OFF",
    "undefinedVars": "ERROR"
  }
}
